<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="tokenGen">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request/>
        <Response>
            <Step>
                <Name>AM-ReturnOauthToken</Name>
            </Step>
        </Response>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPProxyConnection>
        <BasePath>/customerbill-cecpaymentmethods-v1/services/oauth2/token</BasePath>
        <Properties/>
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="noRouteRule"/>
</ProxyEndpoint>